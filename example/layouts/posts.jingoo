<!doctype html>
<html>
  <head>
    {% include "head.jingoo" %}
  </head>
  <body>
    {% include "nav.jingoo" %}
    <h1>{{ page.title }}</h1>
    {{ content }}
    <ul>
      {%- for post in posts
          | sort(compare=compare_dates, reverse=true, attribute="date") -%}
      <li><a href='{{ post.url }}'>{{ post.title }}</a></li>
      {% endfor -%}
    </ul>
  </body>
</html>
