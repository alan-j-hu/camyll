<!doctype html>
<html>
  <head>
    {% include "head.jingoo" %}
  </head>
  <body>
    {% include "nav.jingoo" %}
    <h1>{{ frontmatter.title }}</h1>
    {{ content }}
    <ul>
      {%- for post in pages | sort(reverse=true, attribute="frontmatter.date") -%}
      <li>
        <a href='{{ post.url }}'>{{ post.frontmatter.title }}</a>
        - {{ format_date("%B %_d, %Y", post.frontmatter.date) }}
      </li>
      {% endfor -%}
    </ul>
  </body>
</html>
